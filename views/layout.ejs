<!doctype html>

<html>
<head>
   <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
   <title>Analizador CSV</title>
   <script type="text/html" id='fillTable'>
     <table id="result" class="center">
       <% items.forEach(items, function(name,index) { %>
         <tr class="<%= name.rowClass %>">
           <% name.value.ForEach(name.value, function(cell,index) { %>
             <td><%= cell %></td>
           <% }); %>
         </tr>
       <% }); %>
     </table>
   </script>
</head>

  <body>
    <!--%- title % otr manera tmb con $-->
     <%- index %>
  </body>
</html>
