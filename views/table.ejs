<div>
  <h1><%= title %></h1>
  <div>
    Write a CSV string. Click the table button. The program outputs a table with the specified data.</i><br>
  </div>
  <form action="/table" method="post">
  <table>
      <tr>
        <th>CSV string:</th> <!-- autofocus attribute is HTML5 -->
        <td><textarea name="original" autofocus cols = "80" rows = "5" id="original"></textarea></td>
      </tr>
  </table>
  <button name="button" class="boton" id="button" type="submit">table:</button><br>
  </form>
</div>
<h3>Result:</h3>
<table id="result" class="center">
    <% items.forEach(function(name, index) { %>
        <tr class="<%= name.rowClass %>">
            <% name.value.forEach(function(cell, index){ %>
                <td><%= cell %></td>
            <% }); %>
        </tr>
    <% }); %>
</table>
<a href="/">Atras</a>
